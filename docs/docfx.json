{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/Camunda.Client/Camunda.Client.csproj"],
          "src": ".."
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "filter": "filterConfig.yml",
      "memberLayout": "SeparatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "Camunda C# SDK",
      "_appName": "Camunda.Client",
      "_appFooter": "Camunda 8 Orchestration Cluster API â€” C# SDK",
      "_enableSearch": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/camunda/orchestration-cluster-api-csharp",
        "branch": "main"
      }
    },
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
